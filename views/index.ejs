<!DOCTYPE html>
<html>
<head>
  <title>Stack Overflow Clone</title>
  <link rel="stylesheet" type="text/css" href="/css/styles.css">
</head>
<body>
  <div class="container">
  <h1>All Questions</h1>
  <ul>
    <% questions.forEach(question => { %>
      <div class="question centered">
      <li><a href="/question/<%= question._id %>"><%= question.title %></a> <div class="meta">by <%= question.user.username %></div></li>
    </div>
    </div>
    <% }) %>
  </ul>
  <a href="/question/ask">Ask a new question</a>
  <a href="/auth/logout">Logout</a>
</body>
</html>




